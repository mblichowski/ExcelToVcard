<Window x:Class="ExcelToVcard.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:ExcelToVcard"
        mc:Ignorable="d"
        Title="MainWindow" Height="223" Width="369">
    <DockPanel Margin="10">
        <WrapPanel HorizontalAlignment="Center" DockPanel.Dock="Top" Margin="0,0,0,10">
            <Button x:Name="btnOpenFile" Click="BtnOpenFile_Click" Height="70" Width="244" Margin="10" Content="Open Excel file"/>
            <Button x:Name="btnIdentifyFiles" Click="BtnIdentifyFiles_Click" Height="70" Width="244" Margin="10" Content="Identify vCard images"/>
        </WrapPanel>
    </DockPanel>
</Window>
